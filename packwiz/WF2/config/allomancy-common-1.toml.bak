		#Range: 1 ~ 40
		silver_density = 11
		#Minimum Y Level to Generate Silver
		#Range: 1 ~ 128
		silver_min_y = 16
		#Maximum Y Level to Generate Silver
		#Range: 1 ~ 128
		silver_max_y = 46

	[WorldGen.Tin]
		#Generate Tin Ore
		generate_tin = true
		#Density of Tin Ore
		#Range: 1 ~ 40
		tin_density = 15
		#Minimum Y Level to Generate Tin
		#Range: 1 ~ 128
		tin_min_y = 40
		#Maximum Y Level to Generate Tin
		#Range: 1 ~ 128
		tin_max_y = 80

	[WorldGen.Zinc]
		#Generate Zinc Ore
		generate_zinc = true
		#Density of Zinc Ore
		#Range: 1 ~ 40
		zinc_density = 12
		#Minimum Y Level to Generate Zinc
		#Range: 1 ~ 128
		zinc_min_y = 20
		#Maximum Y Level to Generate Zinc
		#Range: 1 ~ 128
		zinc_max_y = 40

	[WorldGen."Loot Injection"]
		#Add Lerasium to dungeon and other loot tables
		generate_lerasium = true
		#Add Unbreakable Obsidian Daggers to end city and other loot tables
		generate_unbreakable_daggers = true

