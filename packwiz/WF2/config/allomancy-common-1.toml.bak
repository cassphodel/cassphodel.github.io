
#Settings for the mod's added generation
[WorldGen]

	[WorldGen.Aluminum]
		#Generate Aluminum Ore
		generate_aluminum = false
		#Density of Aluminum Ore
		#Range: 1 ~ 40
		aluminum_density = 15
		#Minimum Y Level to Generate Aluminum
		#Range: 1 ~ 128
		aluminum_min_y = 25
		#Maximum Y Level to Generate Aluminum
		#Range: 1 ~ 128
		aluminum_max_y = 55

	[WorldGen.Cadmium]
		#Generate Cadmium Ore
		generate_cadmium = true
		#Density of Cadmium Ore
		#Range: 1 ~ 40
		cadmium_density = 5
		#Minimum Y Level to Cadmium Aluminum
		#Range: 1 ~ 128
		cadmium_min_y = 30
		#Maximum Y Level to Cadmium Aluminum
		#Range: 1 ~ 128
		cadmium_max_y = 50

	[WorldGen.Chromium]
