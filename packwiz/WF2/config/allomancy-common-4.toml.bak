
#Settings for the mod's added generation
[WorldGen]

	[WorldGen.Cadmium]
		#Density of Cadmium Ore
		#Range: 1 ~ 40
		cadmium_density = 5
		#Minimum Y Level to Cadmium Aluminum
		#Range: 1 ~ 128
		cadmium_min_y = 30
		#Maximum Y Level to Cadmium Aluminum
		#Range: 1 ~ 128
		cadmium_max_y = 50
		#Generate Cadmium Ore
		generate_cadmium = true

	[WorldGen.Silver]
		#Generate Silver Ore
		generate_silver = true
		#Maximum Y Level to Generate Silver
		#Range: 1 ~ 128
		silver_max_y = 46
		#Minimum Y Level to Generate Silver
		#Range: 1 ~ 128
		silver_min_y = 16
		#Density of Silver Ore
		#Range: 1 ~ 40
		silver_density = 11

	[WorldGen.Aluminum]
		#Density of Aluminum Ore
		#Range: 1 ~ 40
		aluminum_density = 15
		#Generate Aluminum Ore
		generate_aluminum = true
		#Minimum Y Level to Generate Aluminum
		#Range: 1 ~ 128
		aluminum_min_y = 25
		#Maximum Y Level to Generate Aluminum
		#Range: 1 ~ 128
		aluminum_max_y = 55

	[WorldGen."Loot Injection"]
		#Add Unbreakable Obsidian Daggers to end city and other loot tables
		generate_unbreakable_daggers = false
		#Add Lerasium to dungeon and other loot tables
		generate_lerasium = true

	[WorldGen.Chromium]
		#Maximum Y Level to Generate Chromium
		#Range: 1 ~ 128
		chromium_max_y = 45
		#Generate Chromium Ore
		generate_chromium = true
		#Minimum Y Level to Generate Chromium
		#Range: 1 ~ 128
		chromium_min_y = 20
		#Density of Chromium Ore
		#Range: 1 ~ 40
		chromium_density = 1

	[WorldGen.Lead]
		#Density of Lead Ore
		#Range: 1 ~ 40
		lead_density = 15
		#Minimum Y Level to Generate Lead
		#Range: 1 ~ 128
		lead_min_y = 20
		#Generate Lead Ore
		generate_lead = true
		#Maximum Y Level to Generate Lead
		#Range: 1 ~ 128
		lead_max_y = 40

	[WorldGen.Tin]
		#Density of Tin Ore
		#Range: 1 ~ 40
		tin_density = 15
		#Generate Tin Ore
		generate_tin = true
		#Maximum Y Level to Generate Tin
		#Range: 1 ~ 128
		tin_max_y = 80
		#Minimum Y Level to Generate Tin
		#Range: 1 ~ 128
		tin_min_y = 40

	[WorldGen.Copper]
		#Generate Copper Ore
		generate_copper = false
		#Density of Copper Ore
		#Range: 1 ~ 40
		copper_density = 15
		#Maximum Y Level to Generate Copper
		#Range: 1 ~ 128
		copper_max_y = 70
		#Minimum Y Level to Generate Copper
		#Range: 1 ~ 128
		copper_min_y = 30

	[WorldGen.Zinc]
		#Maximum Y Level to Generate Zinc
		#Range: 1 ~ 128
		zinc_max_y = 40
		#Minimum Y Level to Generate Zinc
		#Range: 1 ~ 128
		zinc_min_y = 20
		#Generate Zinc Ore
		generate_zinc = true
		#Density of Zinc Ore
		#Range: 1 ~ 40
		zinc_density = 12

